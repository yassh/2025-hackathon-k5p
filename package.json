{
  "name": "2025-hackathon-k5p",
  "description": "A blank template to get started with Payload 3.0",
  "version": "1.0.0",
  "dependencies": {
    "@payloadcms/db-postgres": "3.45.0",
    "@payloadcms/next": "3.45.0",
    "@payloadcms/payload-cloud": "3.45.0",
    "@payloadcms/richtext-lexical": "3.45.0",
    "@payloadcms/translations": "^3.45.0",
    "@payloadcms/ui": "3.45.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "next": "15.3.2",
    "next-auth": "5.0.0-beta.29",
    "payload": "3.45.0",
    "payload-authjs": "^0.8.2",
    "react": "19.1.0",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@playwright/test": "1.50.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "fix:prettier": "cross-env NODE_OPTIONS=--no-deprecation prettier './**/*.{js,jsx,ts,tsx}' --write",
    "gen": "run-p gen:* --continue-on-error",
    "gen:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "gen:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "init-db": "run-s init-db:fresh",
    "init-db:fresh": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
    "lint": "run-p lint:*",
    "lint:eslint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:prettier": "cross-env NODE_OPTIONS=--no-deprecation prettier './**/*.{js,jsx,ts,tsx}' --check",
    "lint:tsc": "cross-env NODE_OPTIONS=--no-deprecation tsc --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "type": "module"
}
